{
    "name": "ziumper/multi-classmap",
    "description": "Composer classmap divider plugin",
    "type": "composer-plugin",
    "version": "1.0",
    "require": {
        "composer-plugin-api": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ziumper\\MultiClassMap\\": "src/"
        }
    },
    "autoload-dev": {
        "ps-4": {
            "Ziumper\\MultiClassMap" : "tests/"
        }
    },
    "extra": {
        "class": "Ziumper\\MultiClassMap\\MultiClassMapPlugin"
    }
}
